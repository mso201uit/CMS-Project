@model IEnumerable<CMS_Project.Models.User>

<h1>Users</h1>
<p>
    <a asp-action="Create">Add New User</a>
</p>

<table>
    <thead>
    <tr>
        <th>Id</th>
        <th>Username</th>
        <th>Email</th>
    </tr>
    </thead>
    <tbody>
    @foreach (var user in Model)
    {
    <tr>
        <td>@user.Id</td>
        <td>@user.Username</td>
        <td>@user.Email</td>
    </tr>
    }
    </tbody>
</table>
